{
  "name": "zuper-frontend",
  "description": "Zuper React/Redux frontend",
  "version": "1.1.1",
  "author": "David Revay <dave@zuper.com>",
  "license": "MIT",
  "keywords": [],
  "scripts": {
    "build:html": "node ./scripts/buildHtml.js",
    "prestart": "rimraf dist",
    "start": "webpack-dashboard -c cyan -- node ./scripts/development.js",
    "start:log": "node ./scripts/development.js",
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production node ./scripts/production.js",
    "build:ssr": "cross-env NODE_ENV=production node ./scripts/ssr.js",
    "analyse": "webpack-bundle-analyzer ./reports/stats.json",
    "postbuild": "npm run build:html",
    "build:serve": "http-server dist -p 44300 --cors -o -c10",
    "lint": "standard --verbose | snazzy",
    "copy": "node ./scripts/copyToProd.js",
    "deploy:staging": "node ../zuper-frontend-production/scripts/deploy.js -staging",
    "deploy:prod": "node ../zuper-frontend-production/scripts/deploy.js -prod",
    "serve:prod": "node ../zuper-frontend-production/backend/server.js"
  },
  "devDependencies": {
